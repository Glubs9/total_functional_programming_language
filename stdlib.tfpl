id(a) = a;

plus(0, b) = id(b);
plus(s[a], b) = s[plus(a, b)];

ignore(b) = 0;
multiply(0, b) = ignore(b);
multiply(s[a], b) = plus(b, multiply(a, b));

one(b) = s[0];
power(0, b) = one(b);
power(s[a], b) = multiply(b, power(a, b));

zero() = 0;
pred(0) = zero();
pred(s[a]) = a;

minus(b, 0) = b;
minus(b, s[a]) = pred(minus(b, a));

ack{0, n} = s[n];
ack{s[m], 0} = ack{m, s[0]};
ack{s[m], s[n]} = ack{m, ack{s[m], n}};
